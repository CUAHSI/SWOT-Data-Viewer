{
  "name": "swot_data_viewer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:unit": "cypress run --component",
    "test:unit:dev": "cypress open --component",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "lint-fix": "eslint --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore .",
    "format": "prettier --check src/",
    "format-fix": "prettier --write src/",
    "lint-format": "npm-run-all --parallel lint format",
    "lint-format-fix": "npm-run-all --parallel lint-fix format-fix",
    "precommit": "npm run lint-format",
    "prepush": "npm run lint-format",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@vueuse/core": "^14.1.0",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "chroma-js": "^3.2.0",
    "date-fns": "^4.1.0",
    "esri-leaflet": "^3.0.19",
    "esri-leaflet-geocoder": "^3.1.6",
    "esri-leaflet-vector": "^4.3.2",
    "leaflet": "^1.9.4",
    "leaflet-easybutton": "^2.4.0",
    "marked": "^17.0.1",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "swagger-ui": "^5.30.3",
    "vite-plugin-vuetify": "^2.1.2",
    "vue": "^3.5.25",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.3",
    "vuetify": "^3.11.0"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@rushstack/eslint-patch": "^1.15.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "cypress": "^15.7.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "pm2": "^6.0.14",
    "prettier": "3.7.3",
    "sass": "^1.94.2",
    "start-server-and-test": "^2.1.3",
    "vite": "^7.2.6"
  }
}
