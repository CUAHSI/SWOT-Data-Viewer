{
  "name": "swot_data_viewer",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-pattern .gitignore",
    "lint-fix": "eslint --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-pattern .gitignore",
    "format": "prettier --check src/",
    "format-fix": "prettier --write src/",
    "lint-format": "npm-run-all --parallel lint format",
    "lint-format-fix": "npm-run-all --parallel lint-fix format-fix",
    "precommit": "npm run lint-format",
    "prepush": "npm run lint-format",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "lint-staged": {
    "frontend/**/*.{vue,js,jsx,cjs,mjs}": [
      "npm-run-all --parallel lint-fix format-fix"
    ],
    "api/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@vueuse/core": "^14.1.0",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "chroma-js": "^3.2.0",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "esri-leaflet": "^3.0.19",
    "esri-leaflet-geocoder": "^3.1.6",
    "esri-leaflet-vector": "^4.3.2",
    "leaflet": "^1.9.4",
    "leaflet-easybutton": "^2.4.0",
    "marked": "^17.0.1",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "swagger-ui-dist": "^5.30.2",
    "vite-plugin-raw": "^1.0.3",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vuetify": "^2.1.2",
    "vue": "^3.5.25",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.3",
    "vuetify": "~3.5.0",
    "watchpack": "^2.4.4",
    "webpack": "^5.103.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@rushstack/eslint-patch": "^1.15.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "pm2": "^6.0.14",
    "prettier": "3.7.3",
    "sass": "^1.94.2",
    "start-server-and-test": "^2.1.3",
    "vite": "^7.2.6"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.53.3"
  }
}
