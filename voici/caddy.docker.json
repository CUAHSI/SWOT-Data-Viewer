{
  "apps": {
    "http": {
      "servers": {
        "web": {
          "automatic_https": {
            "disable": true
          },
          "listen": [":8090"],
          "routes": [
            {
              "handle": [
                {
                  "handler": "file_server",
                  "root": "/srv"
                }
              ],
              "match": [
                {
                  "path_regexp": {
                    "name": "asset",
                    "pattern": "\\.(br|css|gz|ico|jpg|js|mp3|mp4|png|svg|md|woff2|ttf)$"
                  }
                }
              ],
              "terminal": true
            }
          ]
        }
      }
    }
  }
}
